{
    "id": "places",
    "endpoint": {
        "url": "https://ldf.fi/sampo-qlever/sparql",
        "useAuth": false,
        "prefixesFile": "SparqlQueriesPrefixes.js",
        "triplestore": "qlever"
    },
    "sparqlQueriesFile": "SparqlQueriesPlaces.js",
    "baseURI": "http://ldf.fi/sampo",
    "URITemplate": "<BASE_URI>/places/<LOCAL_ID>",
    "facetClass": "sch:Place",
    "frontPageImage": "main_page/porvoo.jpg",
    "searchMode": "faceted-search",
    "enableDynamicLanguageChange": true,
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "places": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "placeProperties",
                "pagesize": 20,
                "sortBy": null,
                "sortDirection": null
            },
            "instanceConfig": {
                "propertiesQueryBlock": "placeInstancePageProperties",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    },
                    "proxyLocations": {
                        "tabID": 1,
                        "pageType": "instancePage",
                        "component": "LeafletMap",
                        "tabPath": "map",
                        "tabIcon": "AddLocation",
                        "sparqlQuery": "proxiesMapQuery",
                        "facetClass": "places",
                        "filterTarget": "place",
                        "resultMapper": "mapInstancePagePlaces",
                        "instanceConfig": {
                            "createPopUpContent": "createPopUpContentLetterSampo"
                        },
                        "showExternalLayers": false,
                        "showInstanceCountInClusters": false
                    },
                    "instancePageExport": {
                        "tabID": 3,
                        "component": "Export",
                        "tabPath": "export",
                        "tabIcon": "CloudDownload",
                        "pageType": "instancePage"
                    }
                }
            }
        },
        "placesLocations": {
            "tabID": 1,
            "component": "LeafletMap",
            "tabPath": "map",
            "tabIcon": "AddLocation",
            "sparqlQuery": "placesMapQuery",
            "facetClass": "places",
            "filterTarget": "place",
            "resultMapper": "mapPlaces",
            "instanceConfig": {
                "propertiesQueryBlock": "placePropertiesInfoWindow",
                "relatedInstances": "peopleRelatedTo",
                "createPopUpContent": "createPopUpContentLetterSampo"
            }
        },
        "csvActors": {
            "tabID": 2,
            "component": "ExportCSV",
            "tabPath": "csv",
            "tabIcon": "CloudDownload",
            "facetClass": "places",
            "sparqlQuery": "csvQueryPlaces",
            "filterTarget": "id"
        },
        "export": {
            "tabID": 3,
            "component": "Export",
            "tabPath": "sparql",
            "tabIcon": "ArrowForward",
            "resultClass": "places",
            "facetClass": "places"
        },
        "share": {
            "tabID": 4,
            "component": "ExportLink",
            "tabPath": "share",
            "tabIcon": "IosShare",
            "resultClass": "places",
            "facetClass": "places"
        }
    },
    "maps": {
        "placesLocations": {
            "center": [
                65.0,
                22.5
            ],
            "zoom": 4
        },
        "proxyLocations": {
            "center": [
                65.0,
                22.5
            ],
            "zoom": 4
        }
    },
    "properties": [
        {
            "id": "uri",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "onlyOnInstancePage": true
        },
        {
            "id": "prefLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250
        },
        {
            "id": "proxyPrefLabel",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 150,
            "onlyOnInstancePage": true,
            "showSource": true,
            "sourceExternalLink": false
        },
        {
            "id": "proxyAltLabel",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 150,
            "onlyOnInstancePage": true,
            "showSource": true,
            "sourceExternalLink": false
        },
        {
            "id": "latitude",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 150,
            "onlyOnInstancePage": true,
            "showSource": true,
            "sourceExternalLink": false
        },
        {
            "id": "longitude",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 150,
            "onlyOnInstancePage": true,
            "showSource": true,
            "sourceExternalLink": false
        },
        {
            "id": "sampledLatitude",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 150,
            "disableSort": true
        },
        {
            "id": "sampledLongitude",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 150,
            "disableSort": true
        },
        {
            "id": "peopleBirth",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": true,
            "minWidth": 250,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "peopleDeath",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": true,
            "minWidth": 250,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "relation",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": true,
            "minWidth": 250,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "group",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": true,
            "minWidth": 250,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "website",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250,
            "expandedOnInstancePage": true
        },
        {
            "id": "source",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 250,
            "expandedOnInstancePage": true
        }
    ],
    "facets": {
        "prefLabel": {
            "containerClass": "one",
            "facetType": "text",
            "filterType": "textFilter",
            "sortByPredicate": "skos:prefLabel",
            "textQueryProperty": "skos:prefLabel"
        },
        "relatedPeople": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "((^sch:birthPlace|^sch:deathPlace)/foaf:focus)|(^relations:placeObject/relations:personSubject)",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "(((^sch:birthPlace|^sch:deathPlace)/foaf:focus)|(^relations:placeObject/relations:personSubject))/skos:prefLabel",
            "sortDirection": "desc"
        },
        "peopleBirth": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "^sch:birthPlace/foaf:focus",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "^sch:birthPlace/foaf:focus/skos:prefLabel",
            "sortDirection": "desc"
        },
        "peopleDeath": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "^sch:birthPlace/foaf:focus",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "^sch:deathPlace/foaf:focus/skos:prefLabel",
            "sortDirection": "desc"
        },
        "peopleRelation": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "^relations:placeObject/relations:personSubject",
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "^relations:placeObject/relations:personSubject/skos:prefLabel",
            "sortDirection": "desc"
        },
        "source": {
            "containerClass": "ten",
            "facetType": "list",
            "filterType": "uriFilter",
            "predicate": "^foaf:focus/dce:source",
            "hideUnknownValue": true,
            "searchField": true,
            "sortButton": true,
            "sortBy": "instanceCount",
            "sortByPredicate": "^foaf:focus/dce:source/skos:prefLabel",
            "sortDirection": "desc",
            "pieChartButton": true,
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = 'en')"
        },
        "inconsistencies": {
            "containerClass": "five",
            "facetType": "list",
            "predicate": "sampos:has_inconsistency",
            "filterType": "uriFilter",
            "sortBy": "instanceCount",
            "sortDirection": "desc",
            "pieChartButton": true,
            "sortByPredicate": "sampos:has_inconsistency/skos:prefLabel"
        }
    }
}