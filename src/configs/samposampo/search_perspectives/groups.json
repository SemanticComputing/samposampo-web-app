{
    "id": "groups",
    "endpoint": {
        "url": "https://ldf.fi/sampo-qlever/sparql",
        "useAuth": true,
        "prefixesFile": "SparqlQueriesPrefixes.js",
        "triplestore": "qlever"
    },
    "sparqlQueriesFile": "SparqlQueriesGroups.js",
    "baseURI": "http://ldf.fi/sampo",
    "URITemplate": "<BASE_URI>/groups/<LOCAL_ID>",
    "facetClass": "sch:Organization",
    "frontPageImage": "main_page/aurora.png",
    "searchMode": "faceted-search",
    "enableDynamicLanguageChange": false,
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "maps": {
        "placesActors": {
            "center": [
                65.0,
                22.5
            ],
            "zoom": 4
        }
    },
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "groups": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "groupPropertiesFacetResults",
                "pagesize": 30,
                "sortBy": "pagelinks",
                "sortDirection": "desc"
            },
            "instanceConfig": {
                "propertiesQueryBlock": "groupPropertiesInstancePage",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    },
                    "instancePageExport": {
                        "tabID": 1,
                        "component": "Export",
                        "tabPath": "export",
                        "tabIcon": "CloudDownload",
                        "pageType": "instancePage"
                    }
                }
            }
        },
        "placesActors": {
            "tabID": 1,
            "component": "LeafletMap",
            "tabPath": "map",
            "tabIcon": "AddLocation",
            "sparqlQuery": "groupsMapQuery",
            "facetClass": "groups",
            "filterTarget": "group",
            "resultMapper": "mapPlaces",
            "instanceConfig": {
                "propertiesQueryBlock": "placePropertiesInfoWindow",
                "relatedInstances": "groupsRelatedTo",
                "createPopUpContent": "createPopUpContentLetterSampo"
            }
        },
        "csvActors": {
            "tabID": 2,
            "component": "ExportCSV",
            "tabPath": "csv",
            "tabIcon": "CloudDownload",
            "facetClass": "groups",
            "sparqlQuery": "csvQueryGroups",
            "filterTarget": "id"
        },
        "export": {
            "tabID": 3,
            "component": "Export",
            "tabPath": "sparql",
            "tabIcon": "ArrowForward",
            "resultClass": "groups",
            "facetClass": "groups"
        },
        "share": {
            "tabID": 4,
            "component": "ExportLink",
            "tabPath": "share",
            "tabIcon": "IosShare",
            "resultClass": "groups",
            "facetClass": "groups"
        }
    },
    "properties": [
        {
            "id": "image",
            "valueType": "image",
            "previewImageHeight": 160,
            "makeLink": false,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "hideHeader": true
        },
        {
            "id": "uri",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "onlyOnInstancePage": true
        },
        {
            "id": "prefLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 300
        },
        {
            "id": "altLabel",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 300,
            "showSource": true,
            "sourceExternalLink": false,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "location",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 80,
            "expandedOnInstancePage": true
        },
        {
            "id": "place",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 80,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": false
        },
        {
            "id": "estimated_time",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 300,
            "showSource": true,
            "sourceExternalLink": false,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "category",
            "valueType": "string",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 300,
            "showSource": true,
            "sourceExternalLink": false,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "sentence",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": true,
            "minWidth": 80,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "referenced_person",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": true,
            "minWidth": 80,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "namesake",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 80,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "website",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 80,
            "expandedOnInstancePage": true,
            "disableSort": true
        },
        {
            "id": "external",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 80,
            "expandedOnInstancePage": true,
            "disableSort": true
        }
    ],
    "facets": {
        "prefLabel": {
            "containerClass": "one",
            "facetType": "text",
            "filterType": "textFilter",
            "sortByPredicate": "skos:prefLabel",
            "textQueryProperty": "skos:prefLabel"
        },
        "location": {
            "containerClass": "ten",
            "facetType": "list",
            "predicate": "(^foaf:focus)/sch:location",
            "filterType": "uriFilter",
            "sortBy": "instanceCount",
            "sortDirection": "desc",
            "pieChartButton": true,
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = 'fi')",
            "sortByPredicate": "(^foaf:focus)/sch:location/skos:prefLabel"
        },
        "external": {
            "containerClass": "ten",
            "facetType": "list",
            "predicate": "sampos:in_dataset",
            "filterType": "uriFilter",
            "sortBy": "instanceCount",
            "sortDirection": "desc",
            "pieChartButton": true,
            "sortByPredicate": "sampos:in_dataset/skos:prefLabel",
            "facetLabelFilter": "FILTER(LANG(?prefLabel_) = 'en')"
        },
        "pagelinks": {
            "containerClass": "five",
            "facetType": "integer",
            "predicate": "sampos:pagelinks",
            "filterType": "integerFilter",
            "sortBy": "instanceCount",
            "sortDirection": "desc",
            "pieChartButton": false,
            "sortByPattern": "{ SELECT ?id ?orderBy WHERE { ?id a <FACET_CLASS> ; sampos:pagelinks ?orderBy } }"
        }
    }
}