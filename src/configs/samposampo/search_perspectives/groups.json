{
    "id": "groups",
    "endpoint": {
        "url": "https://ldf.fi/sampo/sparql",
        "useAuth": true,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesGroups.js",
    "baseURI": "http://ldf.fi/sampo",
    "URITemplate": "<BASE_URI>/groups/<LOCAL_ID>",
    "facetClass": "sch:Organization",
    "frontPageImage": "main_page/works-452x262.jpg",
    "searchMode": "faceted-search",
    "enableDynamicLanguageChange": false,
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "maps": {
        "placesActors": {
            "center": [
                65.0,
                22.5
            ],
            "zoom": 4
        },
        "peopleMigrations": {
            "center": [
                65.0,
                22.5
            ],
            "zoom": 4
        }
    },
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "resultClasses": {
        "groups": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "groupPropertiesFacetResults",
                "pagesize": 30,
                "sortBy": "pagelinks",
                "sortDirection": "desc"
            },
            "instanceConfig": {
                "propertiesQueryBlock": "groupPropertiesInstancePage",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    }
                }
            }
        }
    },
    "properties": [
        {
            "id": "image",
            "valueType": "image",
            "previewImageHeight": 160,
            "makeLink": false,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "hideHeader": true
        },
        {
            "id": "uri",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "onlyOnInstancePage": true
        },
        {
            "id": "prefLabel",
            "valueType": "object",
            "makeLink": true,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 300
        },
        {
            "id": "altLabel",
            "valueType": "object",
            "makeLink": false,
            "externalLink": false,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 300,
            "showSource": true,
            "sourceExternalLink": false,
            "onlyOnInstancePage": true,
            "expandedOnInstancePage": true
        },
        {
            "id": "website",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 80,
            "expandedOnInstancePage": true
        },
        {
            "id": "external",
            "valueType": "object",
            "makeLink": true,
            "externalLink": true,
            "sortValues": true,
            "numberedList": false,
            "minWidth": 80,
            "expandedOnInstancePage": true
        }
    ],
    "facets": {
        "pagelinks": {
            "sortByPattern": "{ SELECT ?id ?orderBy WHERE { ?id a <FACET_CLASS> ; sampos:pagelinks ?orderBy } }"
        },
        "prefLabel": {
            "containerClass": "one",
            "facetType": "text",
            "filterType": "textFilter",
            "sortByPredicate": "skos:prefLabel",
            "textQueryProperty": "skos:prefLabel"
        },
        "external": {
            "containerClass": "ten",
            "facetType": "list",
            "predicate": "sampos:in_dataset",
            "filterType": "uriFilter",
            "sortBy": "instanceCount",
            "sortDirection": "desc",
            "pieChartButton": true,
            "sortByPredicate": "sampos:in_dataset/skos:prefLabel"
        }
    }
}